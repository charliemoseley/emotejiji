<% cache 'emoticon_list' do %>
<ul id="emoticon-list" masonry>
  <li ng-repeat="emoticon in emoticonList = (emoticons() | filter:tagFilter)">
    <a href="/emoticons/{{emoticon.id}}"
       class="width-{{emoticon.display_columns}}">
      <pre><span id="{{emoticon.id}}">{{emoticon.text}}</span></pre>
    </a>
  </li>
</ul>
<% end %>